{
  "name": "@sidan-lab/sidan-gov-api",
  "version": "1.0.0",
  "main": "src/server.ts",
  "description": "",
  "basePath": "/",
  "scripts": {
    "dev": "nodemon --watch --env-file=.env",
    "build": "tsc -p .",
    "start": "node ./build/src/server.js",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "clean": "rm -rf .turbo && rm -rf dist && rm -rf node_modules && rm -rf .build",
    "db:format": "prisma format",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:update": "prisma format && prisma db push && prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidan-lab/sidan-gov"
  },
  "author": "sidan-lab",
  "license": "Apache-2.0",
  "homepage": "https://github.com/sidan-lab/sidan-gov",
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^6.0.1",
    "@meshsdk/core": "1.7.10",
    "@prisma/client": "^6.5.0",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "4.17.3",
    "jest-mock-extended": "^4.0.0-beta1",
    "json-schema-to-ts": "^2.9.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "4.17.3",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.7",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "test-node": "^0.0.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  }
